<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.root.alertsapp.Myalerts">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/createdbyme"
        android:layout_alignParentTop="true"
        android:layout_marginTop="64dp"
        android:text="MY ALERTS"
        android:textSize="30sp" />


    <Button
        android:id="@+id/createdbyme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView11"
        android:layout_marginEnd="97dp"
        android:layout_marginTop="95dp"
        android:background="#3f51b4"
        android:text="  CREATED BY ME  "
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/acceptedbyme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView11"
        android:layout_below="@+id/createdbyme"
        android:layout_marginTop="46dp"
        android:background="#3f51b4"
        android:text=" ACCEPTED BY ME "
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold"
        />





</RelativeLayout>
